
def Formula  := dpll.Formula.
def Variable := dpll.Variable.
def or  := dpll.For.
def and := dpll.Fand.
def not := dpll.Fnot.
def var := dpll.Fvar.


a : Variable.
b : Variable.
c : Variable.
d : Variable.
e : Variable.

[x  ] dpll.eq x x --> dpll.T.
[x,y] dpll.eq x y --> dpll.F.

def f : Formula :=
  and
   (not (var a))
   (or
     (and (var a) (var b))
     (not (or (var c) (var a)))).

#EVAL[SNF] f.

#EVAL[SNF] dpll.units_propagate f.

#EVAL[SNF] dpll.solve f.
